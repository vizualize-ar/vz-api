<?xml version="1.0" encoding="utf-8"?>
<configuration>
	<runtime>
		<ThrowUnobservedTaskExceptions enabled="true"/>
		<loadFromRemoteSources enabled="true"/>
		<gcServer enabled="true"/>
		<GCCpuGroup enabled="true"/>
		<Thread_UseAllCpuGroups enabled="true"/>
		<assemblyBinding xmlns="urn:schemas-microsoft-com:asm.v1">
			<dependentAssembly>
				<assemblyIdentity name="Newtonsoft.Json" publicKeyToken="30ad4fe6b2a6aeed" culture="neutral"/>
				<bindingRedirect oldVersion="0.0.0.0-10.0.0.0" newVersion="10.0.0.0"/>
			</dependentAssembly>
		</assemblyBinding>
		<developmentMode developerInstallation="true"/></runtime>
	<appSettings>
		<add key="CosmosDbEnableTcpChannel" value="false"/>
	</appSettings>
	<system.diagnostics>
		<sharedListeners>
			<add name="DocDBFrontend" initializeData="{AA09F651-7EA2-4029-8057-12692E61A9FD}" type="System.Diagnostics.Eventing.EventProviderTraceListener, System.Core, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089"/>
		</sharedListeners>
		<sources>
			<source name="CosmosDBTrace" switchName="SourceSwitch" switchType="System.Diagnostics.SourceSwitch">
				<listeners>
					<remove name="Default"/>
				</listeners>
			</source>
			<source name="PhysicalChargeTracker" switchValue="Information">
				<listeners>
					<remove name="Default"/>
				</listeners>
			</source>
			<source name="DocDBTrace" switchName="SourceSwitch" switchType="System.Diagnostics.SourceSwitch">
				<listeners>
					<remove name="Default"/>
					<add name="DocDBFrontend"/>
				</listeners>
			</source>
			<source name="ClusterVerifier" switchName="SourceSwitch" switchType="System.Diagnostics.SourceSwitch">
				<listeners>
					<remove name="Default"/>
				</listeners>
			</source>
		</sources>
		<switches>
			<add name="SourceSwitch" value="Verbose"/>
		</switches>
		<trace autoflush="true" useGlobalLock="false" indentsize="4">
			<listeners>
				<remove name="Default"/>
			</listeners>
		</trace>
	</system.diagnostics>
	<system.net>
	</system.net>
</configuration>
