<?xml version="1.0"?>
<configuration>
	<startup>
		<supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.0"/>
	</startup>
	<system.diagnostics>
		<sources>
			<source name="DocDBTrace" switchName="SourceSwitch" switchType="System.Diagnostics.SourceSwitch">
				<listeners>
					<remove name="Default"/>
				</listeners>
			</source>
		</sources>
		<switches>
			<add name="SourceSwitch" value="Verbose"/>
		</switches>
		<trace autoflush="true" indentsize="4">
			<listeners>
				<remove name="Default"/>
			</listeners>
		</trace>
	</system.diagnostics>
	<runtime>
		<developmentMode developerInstallation="true"/></runtime>
</configuration>
